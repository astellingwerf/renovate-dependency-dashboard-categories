{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  dependencyDashboard: true,
  dependencyDashboardApproval: true,
  packageRules: [
    {
      matchDepTypes: [
        'devDependencies'
      ],
      dependencyDashboardCategory: "Development Dependencies",
    },
    {
      matchUpdateTypes: "major",
      dependencyDashboardCategory: "Major Updates",
    }
  ]
}